{
  "name": "permissive",
  "version": "0.0.2",
  "description": "A minimalist theme for Ghost.",
  "demo": "https://permissive.andytran.me",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"gulp babel\" \"gulp sass\" \"gulp watch\"",
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "eslint ./src/babel",
    "lint-sass": "sass-lint ./src/sass",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyhqtran/Permissive.git"
  },
  "bugs": {
    "url": "https://github.com/andyhqtran/Permissive/issues"
  },
  "homepage": "https://github.com/andyhqtran/Permissive#readme",
  "contributors": "https://github.com/andyhqtran/Permissive/graphs/contributors",
  "author": {
    "name": "Andy Tran",
    "email": "hello@andytran.me",
    "url": "https://andytran.me"
  },
  "engines": {
    "ghost": ">=0.9.0 <=1.0.0"
  },
  "license": "MIT",
  "screenshots": {
    "desktop": "assets/images/screenshot-desktop.jpg",
    "mobile": "assets/images/screenshot-mobile.jpg"
  },
  "gpm": {
    "type": "theme",
